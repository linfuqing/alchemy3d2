package alchemy.data
{
	import flash.geom.Vector3D;

	public class CSphereData extends CSafeData
	{
		public var radius:Number;
		
		public function CSphereData()
		{
			radius = 0.0;
			
			_orin = new Vector3D;
			
			super();
		}
		
		public function get orin():Vector3D
		{
			return _orin;
		}
		
		protected var _orin:Vector3D;
	}
}