package alchemy.event
{
	import flash.events.Event;
	
	public class CLoaderEvent extends Event
	{
		static public const LOAD_OK:String = "DATA_LOAD_OK";
		static public const LOAD_ERROR:String = "DATA_LOAD_ERROR";
		static public const ALL_LOAD_OK:String = "DATA_ALL_LOAD_OK";
		
		public function CLoaderEvent(type:String, bubbles:Boolean=false, cancelable:Boolean=false)
		{
			super(type, bubbles, cancelable);
		}
	}
}